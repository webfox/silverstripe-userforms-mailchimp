        "silverstripe/userforms": "^5.0",
        "drewm/mailchimp-api": "^2.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name" : "Matthew Hailwood",
            "email": "matthew@webfox.co.nz"
        }
    ],
    "autoload": {
        "psr-4": {
            "WebFox\\UserFormsMailchimp\\": "src/"
        }
    },
    "extra": {
        "installer-name": "userforms-mailchimp"
    }
}
